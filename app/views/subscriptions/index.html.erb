<section class="hero has-background-black-ter">
  <div class="hero-body has-text-centered">
    <div class="column is-half is-offset-one-quarter">
      <h1 class="title has-text-danger">My feed</h1>
       <div class="mb-2">
        <% @subscriptions.each do |hashtag| %>
          <%= change_hashtag_to_link(hashtag.name, style="tag is-dark mb-1") %>
        <% end %>
      </div>
    </div>
  </div>
</section>

<section class="section">
  <div class="cheatsheets">
    <%= render partial: 'cheatsheets/cheatsheet', collection: user_feed %>
  </div>
</section>
